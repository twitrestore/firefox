{
  "manifest_version": 2,
  "name": "TwitRestore",
  "version": "1.0",
  "description": "Get's rid of the bad X bullshit!",
  "permissions": ["<all_urls>", "tabs"],
  "icons": {
    "48": "favicon.ico"
  },
  "content_scripts": [
    {
      "matches": ["*://twitter.com/*"],
      "js": ["contentScript.js"]
    }
  ],
  "browser_action": {
    "default_icon": "favicon.ico"
  },
  "web_accessible_resources": ["favicon.ico"],
  "manifest_version": 2
}
